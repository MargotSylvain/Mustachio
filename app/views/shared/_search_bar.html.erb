<!--binding to object 2.2-->
<%= simple_form_for :movie, url: search_path do |f| %>
  <%= f.input :title, as: :string, class: "form-control", placeholder: 'input title', label: 'title' %>
  <%= f.input :year, as: :integer, class: "form-control", placeholder: 'input year', label: 'year' %>
  <%= f.submit "search", class: "btn btn-primary" %>
<% end %>



